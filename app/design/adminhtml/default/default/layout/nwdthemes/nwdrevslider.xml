<!-- 2024-09-14 Dmitrii Fediuk https://upwork.com/fl/mage2pro
"The names of arguments in `<action method="<methodName>">` calls should match the `methodName`'s arguments":
https://github.com/thehcginstitute-com/m1/issues/680 -->
<layout>
	<nwdrevslider_common>
		<reference name='head'>
			<action method='setTemplate'><template>nwdthemes/revslider/page/head.phtml</template></action>
			<block
				as='thickbox.js'
				name='thickbox.js'
				template='nwdthemes/revslider/head/thickbox.phtml'
				type='adminhtml/template'
			/>
			<action method='addItem'><name>css/nwdthemes/revslider/jui/jquery-ui.css</name><type>skin_css</type></action>
			<action method='addItem'><name>css/nwdthemes/revslider/wp/buttons.css</name><type>skin_css</type></action>
			<action method='addItem'><name>css/nwdthemes/revslider/wp/common.css</name><type>skin_css</type></action>
			<action method='addItem'><name>css/nwdthemes/revslider/wp/dashicons.css</name><type>skin_css</type></action>
			<action method='addItem'><name>css/nwdthemes/revslider/wp/edit.css</name><type>skin_css</type></action>
			<action method='addItem'><name>css/nwdthemes/revslider/wp/forms.css</name><type>skin_css</type></action>
			<action method='addItem'><name>css/nwdthemes/revslider/wp/list-tables.css</name><type>skin_css</type></action>
			<action method='addItem'><name>css/nwdthemes/revslider/admin.css</name><type>skin_css</type></action>
			<action method='addItem'><name>css/nwdthemes/revslider/tipsy.css</name><type>skin_css</type></action>
			<action method='addItem'><name>js/nwdthemes/revslider/farbtastic/farbtastic.css</name><type>skin_css</type></action>
			<action method='addItem'><name>js/nwdthemes/revslider/codemirror/codemirror.css</name><type>skin_css</type></action>
			<action method='addItem'><name>css/nwdthemes/revslider/edit_layers.css</name><type>skin_css</type></action>
			<action method='addItem'><name>js/nwdthemes/revslider/thickbox/thickbox.css</name><type>skin_css</type></action>
			<action method='addItem'><name>css/nwdthemes/revslider/rs/settings.css</name><type>skin_css</type></action>
			<action method='addItem'><name>css/nwdthemes/revslider/revslider.css</name><type>skin_css</type></action>
			<action method='addItem'><name>css/nwdthemes/revslider/dynamic.css</name><type>skin_css</type></action>
			<action method='addItem'><name>css/nwdthemes/revslider/static.css</name><type>skin_css</type></action>
			<action method='addItem'><name>nwdthemes/jquery-1.11.0.min.js</name><type>js</type></action>
			<action method='addItem'><name>nwdthemes/jquery-migrate-1.2.1.min.js</name><type>js</type></action>
			<action method='addItem'><name>nwdthemes/jquery.noconflict.js</name><type>js</type></action>
			<action method='addItem'><name>js/nwdthemes/revslider/jui/jquery-ui.min.js</name><type>skin_js</type></action>
			<action method='addItem'><name>js/nwdthemes/revslider/settings.js</name><type>skin_js</type></action>
			<action method='addItem'><name>js/nwdthemes/revslider/admin.js</name><type>skin_js</type></action>
			<action method='addItem'><name>js/nwdthemes/revslider/jquery.tipsy.js</name><type>skin_js</type></action>
			<action method='addItem'><name>js/nwdthemes/revslider/farbtastic/my-farbtastic.js</name><type>skin_js</type></action>
			<action method='addItem'><name>js/nwdthemes/revslider/codemirror/codemirror.js</name><type>skin_js</type></action>
			<action method='addItem'><name>js/nwdthemes/revslider/codemirror/css.js</name><type>skin_js</type></action>
			<action method='addItem'>
				<name>js/nwdthemes/revslider/dropdownchecklist/ui.dropdownchecklist-1.4-min.js</name>
				<type>skin_js</type>
			</action>
			<action method='addItem'><name>js/nwdthemes/revslider/edit_layers.js</name><type>skin_js</type></action>
			<action method='addItem'><name>js/nwdthemes/revslider/css_editor.js</name><type>skin_js</type></action>
			<action method='addItem'><name>js/nwdthemes/revslider/rev_admin.js</name><type>skin_js</type></action>
			<action method='addItem'>
				<name>js/nwdthemes/revslider/rs/jquery.themepunch.tools.min.js</name>
				<type>skin_js</type></action>
			<action method='addItem'><name>js/nwdthemes/revslider/thickbox/thickbox.js</name><type>skin_js</type></action>
			<action method='addItem'><name>js/nwdthemes/revslider/base64.js</name><type>skin_js</type></action>
		</reference>
	</nwdrevslider_common>
	<nwdrevslider_images_common>
		<reference name='head'>
			<action method='addCss'><name>lib/prototype/windows/themes/magento.css</name></action>
			<action method='addItem'><name>extjs/resources/css/ext-all.css</name><type>js_css</type></action>
			<action method='addItem'><name>prototype/windows/themes/default.css</name><type>js_css</type></action>
			<action method='addJs'><name>extjs/ext-tree.js</name></action>
			<action method='addJs'><name>extjs/fix-defer.js</name></action>
			<action method='addJs'><name>extjs/ext-tree-checkbox.js</name></action>
			<action method='addJs'><name>lib/flex.js</name></action>
			<action method='addJs'><name>lib/FABridge.js</name></action>
			<action method='addJs'><name>mage/adminhtml/flexuploader.js</name></action>
			<action method='addItem'><name>js/nwdthemes/revslider/browser.js</name><type>skin_js</type></action>
		</reference>
	</nwdrevslider_images_common>
	<adminhtml_nwdrevslider_index>
		<update handle='adminhtml_nwdrevslider_sliders'/>
	</adminhtml_nwdrevslider_index>
	<adminhtml_nwdrevslider_sliders>
		<update handle='nwdrevslider_common'/>
		<reference name='content'>
			<block
				as='nwdrevslider.masterview'
				name='nwdrevslider.masterview'
				template='nwdthemes/revslider/masterview.phtml'
				type='nwdrevslider/adminhtml_masterview'
			>
				<block as='view' name='view' template='nwdthemes/revslider/sliders.phtml' type='nwdrevslider/adminhtml_sliders'>
					<block
						as='release_log'
						name='release_log'
						template='nwdthemes/revslider/html/release_log.html'
						type='adminhtml/template'
					/>
					<block as='sliders_list' name='sliders_list' template='nwdthemes/revslider/templates/sliders_list.phtml'
						type='nwdrevslider/adminhtml_sliders_list'/>
					<block
						as='sliders_list_template'
						name='sliders_list_template'
						template='nwdthemes/revslider/templates/sliders_list.phtml'
						type='nwdrevslider/adminhtml_sliders_list'
					/>
					<block
						as='dialog_preview_slider'
						name='dialog_preview_slider'
						template='nwdthemes/revslider/templates/dialog_preview_slider.phtml'
						type='adminhtml/template'
					/>
				</block>
				<block
					as='general_settings_dialog'
					name='general_settings_dialog'
					template='nwdthemes/revslider/system/general_settings_dialog.phtml'
					type='nwdrevslider/adminhtml_block_template'
				/>
			</block>
		</reference>
	</adminhtml_nwdrevslider_sliders>
	<adminhtml_nwdrevslider_slider>
		<update handle='nwdrevslider_common'/>
		<reference name='content'>
			<block
				as='nwdrevslider.masterview'
				name='nwdrevslider.masterview'
				template='nwdthemes/revslider/masterview.phtml'
				type='nwdrevslider/adminhtml_masterview'
			>
				<block as='view' name='view' type='nwdrevslider/adminhtml_slider_edit'>
					<block
						as='slider_toolbox'
						name='slider_toolbox'
						template='nwdthemes/revslider/templates/slider_toolbox.phtml'
						type='adminhtml/template'
					/>
					<block
						as='slider_api'
						name='slider_api'
						template='nwdthemes/revslider/templates/slider_api.phtml'
						type='adminhtml/template'
					/>
					<block
						as='slider_css_js'
						name='slider_css_js'
						template='nwdthemes/revslider/templates/slider_css_js.phtml'
						type='adminhtml/template'
					/>
					<block
						as='dialog_preview_slider'
						name='dialog_preview_slider'
						template='nwdthemes/revslider/templates/dialog_preview_slider.phtml'
						type='adminhtml/template'
					/>
				</block>
			</block>
		</reference>
	</adminhtml_nwdrevslider_slider>
	<adminhtml_nwdrevslider_slidertemplate>
		<update handle='nwdrevslider_common'/>
		<reference name='content'>
			<block
				as='nwdrevslider.masterview'
				name='nwdrevslider.masterview'
				template='nwdthemes/revslider/masterview.phtml'
				type='nwdrevslider/adminhtml_masterview'
			>
				<block as='view' name='view' type='nwdrevslider/adminhtml_slider_template_edit'>
					<block
						as='slider_toolbox'
						name='slider_toolbox'
						template='nwdthemes/revslider/templates/slider_toolbox.phtml'
						type='adminhtml/template'
					/>
					<block
						as='slider_api'
						name='slider_api'
						template='nwdthemes/revslider/templates/slider_api.phtml'
						type='adminhtml/template'
					/>
					<block
						as='slider_css_js'
						name='slider_css_js'
						template='nwdthemes/revslider/templates/slider_css_js.phtml'
						type='adminhtml/template'
					/>
					<block
						as='dialog_preview_slider'
						name='dialog_preview_slider'
						template='nwdthemes/revslider/templates/dialog_preview_slider.phtml'
						type='adminhtml/template'
					/>
				</block>
			</block>
		</reference>
	</adminhtml_nwdrevslider_slidertemplate>
	<adminhtml_nwdrevslider_slides>
		<update handle='nwdrevslider_common'/>
		<update handle='nwdrevslider_images_common'/>
		<reference name='content'>
			<block
				as='nwdrevslider.masterview'
				name='nwdrevslider.masterview'
				template='nwdthemes/revslider/masterview.phtml'
				type='nwdrevslider/adminhtml_masterview'
			>
				<block as='view' name='view' type='nwdrevslider/adminhtml_slides'>
					<block
						as='slides_list'
						name='slides_list'
						template='nwdthemes/revslider/templates/slides_list.phtml'
						type='adminhtml/template'
					/>
					<block
						as='slides_list_posts'
						name='slides_list_posts'
						template='nwdthemes/revslider/templates/slides_list_posts.phtml'
						type='adminhtml/template'
					/>
					<block
						as='dialog_preview_slide'
						name='dialog_preview_slide'
						template='nwdthemes/revslider/templates/dialog_preview_slide.phtml'
						type='adminhtml/template'
					/>
				</block>
			</block>
		</reference>
	</adminhtml_nwdrevslider_slides>
	<adminhtml_nwdrevslider_slide>
		<update handle='nwdrevslider_common'/>
		<update handle='nwdrevslider_images_common'/>
		<reference name='content'>
			<block
				as='nwdrevslider.masterview'
				name='nwdrevslider.masterview'
				template='nwdthemes/revslider/masterview.phtml'
				type='nwdrevslider/adminhtml_masterview'
			>
				<block
					as='view'
					name='view'
					template='nwdthemes/revslider/templates/slide.phtml'
					type='nwdrevslider/adminhtml_slide'
				>
					<block as='edit_layers' name='edit_layers' template='nwdthemes/revslider/templates/edit_layers.phtml'
						type='adminhtml/template'/>
					<block as='dialog_preview_slide' name='dialog_preview_slide' template='nwdthemes/revslider/templates/dialog_preview_slide.phtml'
						type='adminhtml/template'/>
					<block as='dialog_preview_slider' name='dialog_preview_slider' template='nwdthemes/revslider/templates/dialog_preview_slider.phtml'
						type='adminhtml/template'/>
					<block as='video_dialog' name='video_dialog' template='nwdthemes/revslider/system/video_dialog.phtml'
						type='adminhtml/template'/>
				</block>
			</block>
		</reference>
	</adminhtml_nwdrevslider_slide>
	<adminhtml_images_index>
		<remove name='footer'/>
		<remove name='head'/>
		<reference name='left'>
			<block name='wysiwyg_images.js' template='cms/browser/js.phtml' type='adminhtml/cms_wysiwyg_images_content'/>
			<block name='wysiwyg_images.tree' template='cms/browser/tree.phtml' type='adminhtml/cms_wysiwyg_images_tree'/>
		</reference>
		<reference name='content'>
			<block name='wysiwyg_images.content' template='cms/browser/content.phtml' type='adminhtml/cms_wysiwyg_images_content'>
				<block name='wysiwyg_images.uploader' template='cms/browser/content/uploader.phtml'
					type='adminhtml/cms_wysiwyg_images_content_uploader'/>
				<block name='wysiwyg_images.newfolder' template='cms/browser/content/newfolder.phtml'
					type='adminhtml/cms_wysiwyg_images_content_newfolder'/>
			</block>
		</reference>
	</adminhtml_images_index>
	<adminhtml_images_contents>
		<block name='wysiwyg_images.files' output='toHtml'
			template='cms/browser/content/files.phtml' type='nwdrevslider/adminhtml_images_content_files'/>
	</adminhtml_images_contents>
	<adminhtml_nwdrevslider_error>
		<reference name='content'>
			<block as='slider_error' name='slider_error' template='nwdthemes/revslider/templates/error.phtml'
				type='adminhtml/template'/>
		</reference>
	</adminhtml_nwdrevslider_error>
</layout>