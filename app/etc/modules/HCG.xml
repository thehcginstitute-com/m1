<?xml version='1.0'?>
<config>
	<modules>
		<HCG_Core>
			<active>true</active>
			<codePool>local</codePool>
		</HCG_Core>
<!-- 2024-01-06
"Port the modifications of `app/code/core/Mage/Page/Block/Html/Head.php` to Magento 1.9.4.5":
https://github.com/thehcginstitute-com/m1/issues/97 -->
		<HCG_Page>
			<active>true</active>
			<codePool>local</codePool>
			<depends>
				<HCG_Core/>
				<Mage_Page/>
			</depends>
		</HCG_Page>
<!-- 2024-01-03
"Transfer payment handling from `HCG_Core` to `HCG_Payment`": https://github.com/thehcginstitute-com/m1/issues/82 -->
		<HCG_Payment>
			<active>true</active>
			<codePool>local</codePool>
			<depends>
				<HCG_Core/>
				<Mage_Payment/>
			</depends>
		</HCG_Payment>
		<HCG_WP>
			<active>true</active>
			<codePool>local</codePool>
			<depends>
				<HCG_Core/>
			</depends>
		</HCG_WP>
	</modules>
</config>